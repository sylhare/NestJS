{
  "name": "typescript-seed",
  "version": "1.0.0",
  "description": "template project for typescript",
  "scripts": {
    "build": "nest build",
    "test": "jest --coverage",
    "test:fail": "jest 2>&1 | grep '‚óè'",
    "test:e2e": "jest --config ./jest.config.e2e.ts",
    "test:unit": "jest --config ./jest.config.unit.ts",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --fix src tests --ext=.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "graphql:generate": "ts-node src/graphql/schema-first/schemas/generate-typings.ts",
    "graphql:print": "ts-node src/graphql/code-first/schemas/generate-schema.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/typeorm/datasource.ts"
  },
  "eslintIgnore": [
    "migrations"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-morph": "^21.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "pg": "^8.11.5",
    "rate-limiter-flexible": "^5.0.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  }
}
